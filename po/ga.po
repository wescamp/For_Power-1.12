#: For_Power/scenarios/05_A_Mage.cfg:5
msgid "A Mage"
msgstr ""

#. [side]
#: For_Power/scenarios/05_A_Mage.cfg:55
msgid "Strange Mage"
msgstr ""

#. [side]
#: For_Power/scenarios/05_A_Mage.cfg:66
msgid "Assassin"
